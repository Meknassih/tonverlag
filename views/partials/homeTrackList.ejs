<% for( let index = 0; index < tracks.length; index++ ) { %>
<div class="card card-track text-center">
    <div class="card-body">
        <h5 class="card-title"><%= tracks[index].displayName %></h5>
        <!-- <p class="card-text text-secondary">Uploaded by </p> -->
        <button class="btn btn-success btn-play" data-filename="<%= tracks[index].fileName %>" data-displayname="<%= tracks[index].displayName %> ">Play</button>
        <button href="#" class="btn btn-secondary">Share</button>
    </div>
    <div class="card-footer text-muted">
        <span>
            Added <%= new Date(tracks[index].createdAt).toLocaleString() %>
        </span>
        |
        <span>
            Updated <%= new Date(tracks[index].updatedAt).toLocaleString() %>
        </span>
    </div>
</div>
<% } %>